{
  "name": "read-multiple-files",
  "version": "1.1.0",
  "description": "Read multiple files asynchronously",
  "repository": "shinnn/read-multiple-files",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/read-multiple-files/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "fs",
    "multiple",
    "parallel",
    "file",
    "files",
    "read",
    "async",
    "asynchronous",
    "asynchronously"
  ],
  "dependencies": {
    "graceful-fs": "^3.0.5",
    "run-parallel": "^1.0.0",
    "strip-bom": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^0.12.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.10.0",
    "tap-spec": "^2.1.2",
    "tape": "^3.4.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
